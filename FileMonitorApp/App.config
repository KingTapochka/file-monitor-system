<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- Адрес сервера FileMonitorService -->
    <add key="ServerAddress" value="http://dc03:5000" />
    <!-- Горячая клавиша для проверки файла (формат: Modifier+Key) -->
    <add key="HotKey" value="Win+Shift+F" />
    <!-- Автозапуск при старте Windows -->
    <add key="AutoStart" value="true" />
  </appSettings>
</configuration>
